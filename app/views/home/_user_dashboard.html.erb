<div class="grid grid-cols-2 mt-10 gap-5">
  <div id="chart-container"></div>
  <div id="parking-time-chart"></div>
</div>

<script>
    // Client
/* Chart data */
$(document).ready(function() {
      $.ajax({
        url: '/chart_data',
        method: 'GET',
        dataType: 'json',
        success: function(data) {
          // Render the chart using ApexCharts
          var options = {
            chart: {
              type: 'line',
            },
            series: [{
              name: 'Bookings',
              data: data.map(function(item) {
                return {
                  x: new Date(item.date),
                  y: item.count
                };
              })
            }],
            xaxis: {
              type: 'datetime',
              title: {
                text: 'Date'
              }
            },
            yaxis: {
              title: {
                text: 'Bookings'
              }
            },
            title: {
              text: 'Bookings Over Time',
              align: 'center'
            }
          };

          var chart = new ApexCharts(document.querySelector("#chart-container"), options);
          chart.render();
        }
      });
    });


/* Parking time data */
$(document).ready(function() {
      $.ajax({
        url: '/parking_data',
        method: 'GET',
        dataType: 'json',
        success: function(data) {
          // Render the chart using ApexCharts
          var options = {
            chart: {
              type: 'bar'
            },
            series: [{
              name: 'Parking Time',
              data: data.data
            }],
            plotOptions: {
              bar: {
                borderRadius: 10,
              }
            },
            xaxis: {
              categories: data.categories
            },
            yaxis: {
              title: {
                text: 'Total Parking Time (minutes)' // Thêm đơn vị cho trục dọc
              }
            },
            dataLabels: {
              enabled: true
            },
            title: {
              text: 'Parking Time Over the Week'
            }
          };

          var chart = new ApexCharts(document.querySelector("#parking-time-chart"), options);
        chart.render();
        }
      });
    });
</script>