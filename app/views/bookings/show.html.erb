<div class="p-4 sm:ml-64">
  <div class="p-4 dark:border-gray-700 mt-14">
    <div class="flex items-center justify-between">
      <h1 class="flex-1 font-bold text-3xl">Booking</h1>
  
      <div class="flex-wrap lg:flex items-center lg:space-x-3">
        <%= link_to bookings_path, class: button_class(theme: "transparent") do %>
        All Bookings
        <% end %>
        <%= link_to edit_booking_path(@booking), class: button_class(theme:"primary") do %>
        Edit
        <% end %>
      </div>
    </div>
  
    <ul class="text-slate-700">
        <li>
          <p class="font-semibold">Parking slot:</p>
          <p><%= @booking.parking_slot %></p>
        </li>
        <li>
          <p class="font-semibold">Vehicle:</p>
          <p><%= @booking.vehicle %></p>
        </li>
        <li>
          <p class="font-semibold">User:</p>
          <p><%= @booking.user %></p>
        </li>
        <li>
          <p class="font-semibold">Starttime:</p>
          <p><%= @booking.startTime %></p>
        </li>
        <li>
          <p class="font-semibold">Endtime:</p>
          <p><%= @booking.endTime %></p>
        </li>
        <li>
          <p class="font-semibold">Status:</p>
          <p><%= @booking.status %></p>
        </li>
    </ul>
  </div>
</div>
