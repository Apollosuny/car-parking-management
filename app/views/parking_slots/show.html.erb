<div class="p-4 sm:ml-64">
  <div class="p-4 border-2 border-gray-200 border-dashed rounded-lg dark:border-gray-700 mt-14">
    <div class="flex items-center justify-between">
      <h1 class="flex-1 font-bold text-3xl">Parking_slot</h1>
  
      <div class="flex-wrap lg:flex items-center lg:space-x-3">
        <%= link_to parking_slots_path, class: button_class(theme: "transparent") do %>
        All Parking_slots
        <% end %>
        <%= link_to edit_parking_slot_path(@parking_slot), class: button_class(theme:"primary") do %>
        Edit
        <% end %>
      </div>
    </div>
  
    <ul class="text-slate-700">
        <li>
          <p class="font-semibold">Slotnumber:</p>
          <p><%= @parking_slot.slotNumber %></p>
        </li>
        <li>
          <p class="font-semibold">Status:</p>
          <p><%= @parking_slot.status %></p>
        </li>
    </ul>
  </div>
</div>
